{
  "manifest_version": 2,
  "name": "Parks and Rec Speak",
  "version": "1.0",
  "description": "Read Pawnee-style",
  "icons":  { "128": "icon.png" },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": 
  [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ]
}
